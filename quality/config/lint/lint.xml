<?xml version="1.0" encoding="UTF-8"?>
<lint>
    <!-- Unused will be removed on release -->
    <issue id="UnusedResources" severity="ignore"/>

    <!-- Using the material icons provided from Google -->
    <issue id="IconExpectedSize" severity="ignore"/>

    <!-- Not using Google App Indexing -->
    <issue id="GoogleAppIndexingApiWarning" severity="ignore"/>

    <issue id="GoogleAppIndexingWarning" severity="ignore"/>

    <!-- Do not fail build when new dependency is available -->
    <issue id="GradleDependency" severity="ignore"/>

    <!-- Do not fail build when new version dependency is available -->
    <issue id="NewerVersionAvailable" severity="ignore"/>

    <!-- Avoid content description warning for images -->
    <issue id="ContentDescription" severity="ignore"/>

    <!-- Avoid long vector path for drawables -->
    <issue id="VectorPath" severity="ignore"/>
</lint>
